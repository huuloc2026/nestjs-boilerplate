# Application
NODE_ENV=development
PORT=3000

# Database (PostgreSQL)
DATABASE_URL="postgresql://user:password@localhost:5432/nestdb?schema=public"

# JWT Authentication
JWT_SECRET="yourSuperSecretJWTKeyThatIsVeryLongAndComplex"
JWT_EXPIRES_IN=1h

# Redis Caching
REDIS_HOST=localhost
REDIS_PORT=6379

# Social Authentication (Example for Google)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
# Add similar entries for GitHub, Facebook, Twitter, Microsoft, LinkedIn, GitLab, Discord, Apple, Stack

# Multi-Cloud Storage (Choose and fill as needed)
# AWS S3
AWS_S3_REGION=your_aws_s3_region
AWS_S3_ACCESS_KEY_ID=your_aws_s3_access_key_id
AWS_S3_SECRET_ACCESS_KEY=your_aws_s3_secret_access_key
AWS_S3_BUCKET_NAME=your_aws_s3_bucket_name

# Alibaba Cloud OSS
ALIYUN_OSS_REGION=your_aliyun_oss_region
ALIYUN_OSS_ACCESS_KEY_ID=your_aliyun_oss_access_key_id
ALIYUN_OSS_ACCESS_KEY_SECRET=your_aliyun_oss_access_key_secret
ALIYUN_OSS_BUCKET_NAME=your_aliyun_oss_bucket_name

# Tencent Cloud COS
TENCENT_COS_REGION=your_tencent_cos_region
TENCENT_COS_SECRET_ID=your_tencent_cos_secret_id
TENCENT_COS_SECRET_KEY=your_tencent_cos_secret_key
TENCENT_COS_BUCKET_NAME=your_tencent_cos_bucket_name

# Email (Nodemailer)
NODEMAILER_HOST=smtp.example.com
NODEMAILER_PORT=587
NODEMAILER_USER=your_email@example.com
NODEMAILER_PASS=your_email_password

# Payments (Stripe)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret

# BetterAuth (if used)
BETTERAUTH_API_KEY=your_betterauth_api_key